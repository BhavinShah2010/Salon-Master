include header
doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='author', content='')
    title Home | Salon Master
    link(href='/css/bootstrap.min.css', rel='stylesheet')
    link(href='/css/font-awesome.min.css', rel='stylesheet')
    link(href='/css/prettyPhoto.css', rel='stylesheet')
    link(href='/css/price-range.css', rel='stylesheet')
    link(href='/css/animate.css', rel='stylesheet')
    link(href='/css/main.css', rel='stylesheet')
    link(href='/css/responsive.css', rel='stylesheet')
    script(src='/js/jquery_min.js')
    script(src='/js/jquery.js')
    script(type='text/javascript', src='/js/home_page.js')
    //if lt IE 9
      script(src='/js/html5shiv.js')
      script(src='/js/respond.min.js')
    link(rel='shortcut icon', href='/images/ico/favicon.ico')
    link(rel='apple-touch-icon-precomposed', sizes='144x144', href='/images/ico/apple-touch-icon-144-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='114x114', href='/images/ico/apple-touch-icon-114-precomposed.png')
    link(rel='apple-touch-icon-precomposed', sizes='72x72', href='/images/ico/apple-touch-icon-72-precomposed.png')
    link(rel='apple-touch-icon-precomposed', href='/images/ico/apple-touch-icon-57-precomposed.png')
  // /head
  script(type='text/javascript').
    $(document).ready(function(){
    document.getElementById("search-text").value = "";
    });
  body
    section#slider
      // slider
      .container
        .row
          .col-sm-12
            #slider-carousel.carousel.slide(data-ride='carousel')
              ol.carousel-indicators
                li.active(data-target='#slider-carousel', data-slide-to='0')
                li(data-target='#slider-carousel', data-slide-to='1')
                li(data-target='#slider-carousel', data-slide-to='2')
                li(data-target='#slider-carousel', data-slide-to='3')
                li(data-target='#slider-carousel', data-slide-to='4')
              .carousel-inner
                .item.active
                  .container
                    .row
                      .col-sm-12
                        #slider-carousel.carousel.slide(data-ride='carousel')
                          ol.carousel-indicators
                            li.active(data-target='#slider-carousel', data-slide-to='0')
                            li(data-target='#slider-carousel', data-slide-to='1')
                            li(data-target='#slider-carousel', data-slide-to='2')
                            li(data-target='#slider-carousel', data-slide-to='3')
                            li(data-target='#slider-carousel', data-slide-to='4')
                          .carousel-inner
                  .col-sm
                    img.girl.img-responsive(src='../images/home/Slideshow_Images/Image5.jpg', alt='')
                .item
                  .col-sm
                    img.girl.img-responsive(src='../images/home/Slideshow_Images/Image2.jpg', alt='')
                .item
                  .col-sm
                    img.girl.img-responsive(src='../images/home/Slideshow_Images/Image3.jpg', alt='')
                .item
                  .col-sm
                    img.girl.img-responsive(src='../images/home/Slideshow_Images/Image4.jpg', alt='')
                .item
                  .col-sm
                    img.girl.img-responsive(src='../images/home/Slideshow_Images/Image1.jpg', alt='')
              a.left.control-carousel.hidden-xs(href='#slider-carousel', data-slide='prev')
                i.fa.fa-angle-left
              a.right.control-carousel.hidden-xs(href='#slider-carousel', data-slide='next')
                i.fa.fa-angle-right
    // /slider
    section
      .row
        .col-sm-3
          .left-sidebar
            #accordian.panel-group.category-products
              // category-productsr
              .panel-heading
                h2.panel-title
                  a(data-toggle='collapse', data-parent='#accordian', href='#Services')
                    span.badge.pull-right
                      i.fa.fa-plus
                    |                 Services
              #Services.panel-collapse.collapse
                .panel.panel-default
                  .panel-heading
                    h4.panel-title
                      a(data-toggle='collapse', data-parent='#accordian', href='#servicesForBody')
                        span.badge.pull-right
                          i.fa.fa-plus
                        |                       Body
                  #servicesForBody.panel-collapse.collapse
                    .panel-body
                      ul
                        li
                          | Body Spa 
                          input#Body_spa(type='checkbox', name='services', value='BodySpa', onchange='body_spa_filter (this)')
                        li
                          | Body Treatment
                          input(type='checkbox', name='services', value='BodyTreatment', onchange='body_treatment_filter (this)')
                        li
                          | Waxing 
                          input(type='checkbox', name='services', value='Waxing', onchange='waxing_filter (this)')
                        li
                          | Laser Hair Removeal
                          input(type='checkbox', name='services', value='LaserHair', onchange='Laser_Hair_filter (this)')
                        li
                          | Body Scrub 
                          input(type='checkbox', name='services', value='BodyScrup', onchange='Body_scrub(this)')
                .panel.panel-default
                  .panel-heading
                    h4.panel-title
                      a(data-toggle='collapse', data-parent='#accordian', href='#Hair')
                        span.badge.pull-right
                          i.fa.fa-plus
                        |                       Hair
                  #Hair.panel-collapse.collapse
                    .panel-body
                      ul
                        li
                          | Hair Smoothning
                          input(type='checkbox', name='HairSmoothning', onchange='hair_smooth(this)')
                        li
                          | Hair Cutting
                          input(type='checkbox', name='HairCutting', onchange='hair_cut(this)')
                        li
                          | Hair Wash
                          input(type='checkbox', name='HairWash', onchange='hair_wash (this)')
                        li
                          | Straightenning
                          input(type='checkbox', name='Straightenning', onchange='straight(this)')
                        li
                          | Hair Treatment
                          input(type='checkbox', name='Hair Treatment', onchange='hair_treatment(this)')
                .panel.panel-default
                  .panel-heading
                    h4.panel-title
                      a(data-toggle='collapse', data-parent='#accordian', href='#Nail')
                        span.badge.pull-right
                          i.fa.fa-plus
                        |                       Nail
                  #Nail.panel-collapse.collapse
                    .panel-body
                      ul
                        li
                          | Nail Art
                          input(type='checkbox', name='NailArt', onchange='nail_art(this)')
                        li
                          | Padicure
                          input(type='checkbox', name='Padicure', onchange='manicure_padicure(this)')
                        li
                          | Menicure
                          input(type='checkbox', name='Menicure', onchange='manicure_padicure(this)')
                        li
                          | Nail Extension 
                          input(type='checkbox', name='NailExtension', onchange='nail_extension(this)')
                .panel.panel-default
                  .panel-heading
                    h4.panel-title
                      a(data-toggle='collapse', data-parent='#accordian', href='#face')
                        span.badge.pull-right
                          i.fa.fa-plus
                        |                       face
                  #face.panel-collapse.collapse
                    .panel-body
                      ul
                        li
                          | Facial 
                          input(type='checkbox', name='Facial', onchange='facial(this)')
                        li
                          | Bleach 
                          input(type='checkbox', name='Bleach', onchange='bleach(this)')
                        li
                          | Threading
                          input(type='checkbox', name='Threading', onchange='threading(this)')
                        li
                          | Clean-up
                          input(type='checkbox', name='CleanUp', onchange='cleanup(this)')
                .panel.panel-default
                  .panel-heading
                    h4.panel-title
                      a(data-toggle='collapse', data-parent='#accordian', href='#Make-up')
                        span.badge.pull-right
                          i.fa.fa-plus
                        |                       Make-up
                  #Make-up.panel-collapse.collapse
                    .panel-body
                      ul
                        li
                          | Marriage
                          input(type='checkbox', name='Marriage', onchange='marriage(this)')
                        li
                          | Party
                          input(type='checkbox', name='Party', onchange='party(this)')
              .panel-heading
                h2.panel-title
                  a(data-toggle='collapse', data-parent='#accordian', href='#Offers')
                    span.badge.pull-right
                      i.fa.fa-plus
                    |                       Offers
              #Offers.panel-collapse.collapse
                .panel.panel-default(align='left')
                  .panel-heading(align='left')
                    h4.panel-title
                      a(data-toggle='collapse', data-parent='#accordian', href='#Dove')
                        #main_offers
                          .images
                            img(src='../images/offers/1.png', width='200', height='200')
                          hr
                          .images
                            img(src='../images/offers/1.png', width='200', height='200')
        .container
          .col-sm-9.padding-right
            .features_items
              // features_items
              h2.title.text-center
                | Salons in 
                span#text_value  
              each salon in salonData
                .col-sm-4
                  .product-image-wrapper
                    #first_image.single-products
                      .productinfo.text-center
                        img(src='/images/home/product1.jpg', alt='')
                        h2
                          a(href='a.html') #{salon.name}
                        p #{salon.type} salons
                        p #{salon.address.area}
                          | #{salon.address.city}
                        p #{salon.address.state}-#{salon.address.zipcode}
                        p Contact No: #{salon.phoneNo}
                      .product-overlay
                        .overlay-content
                          // features_items
    .header_top
      .container
        .row
          .col-sm-6
            .contactinfo
              ul.nav.nav-pills
                li &copy Copyright @ Salon Master
    script(src='/js/jquery.js')
    script(src='/js/bootstrap.min.js')
    script(src='/js/jquery.scrollUp.min.js')
    script(src='/js/price-range.js')
    script(src='/js/jquery.prettyPhoto.js')
    script(src='/js/main.js')
    script(type='text/javascript', src='/js/jquery_min.js')
    #disablingDiv
      #location_div
        #search-box
          input#search-text(type='text', name='Search_City', placeholder='Search City')
          input#search-button(type='button', onclick='search_btn(this)', value='Search')
          span Search
